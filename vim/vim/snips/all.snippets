# 时间
snippet date
`!v strftime("%Y-%m-%d %H:%M:%S")`
endsnippet

snippet datez
`!v strftime("%Y-%m-%d %H:%M:%S %z")`
endsnippet

snippet ts
vim: set ts=${1:4} sw=${2:$1}${3: MORE OPTIONS}:$0
endsnippet

snippet ddate
`!v strftime("%B %d, %Y")`
endsnippet

snippet sdate
`!v strftime("%Y.%m.%d")`
endsnippet



# 标记
snippet \td
# -----------
# | TODO: ${1}
# -----------
endsnippet

snippet \mk
# -----------
# | MARK: ${1}
# -----------
endsnippet
